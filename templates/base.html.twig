<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>

        {# Wiadomo≈õci FlashMessage #}
        {% for message in app.flashes("success") %}
            <p>{{ message }}</p>
        {% endfor %}

        {% for message in app.flashes("error") %}
            <p>{{ message }}</p>
        {% endfor %}

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}


    </body>
</html>
